{
  "dockerd_socket": "/var/run/docker.sock",
  "containerd_socket": "/run/containerd/containerd.sock",
  "ogred_socket": "/var/run/ogred.sock",
  "ogred_pid": "/etc/ogre/ogred.pid",
  "log": {
    "level": "trace",
    "file": "/var/log/ogred.log",
    "silent": false,
    "report_caller": false
  },
  "services": [
    {
      "type": "docker",
      "log": {
        "level": "trace",
        "file": "/var/log/service.log",
        "silent": false,
        "report_caller": false
      }
    }
  ]
}